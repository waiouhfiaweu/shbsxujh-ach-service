                                    <destFileName>cobis-ach-job-processing-lambda.jar</destFileName>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.cobis.ach.service</groupId>
                                    <artifactId>cobis-ach-batch-transform-call-service-lambda</artifactId>
                                    <version>${ach.service.version}</version>
                                    <overWrite>true</overWrite>
