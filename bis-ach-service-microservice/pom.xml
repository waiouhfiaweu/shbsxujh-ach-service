            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>2.10</version>
                <executions>
                    <execution>
                        <id>copy-dependencies</id>
                        <phase>install</phase>
                        <goals>
                            <goal>copy</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>com.cobis.ach.service</groupId>
                                    <artifactId>cobis-ach-job-processing-lambda</artifactId>
                                    <version>${ach.service.version}</version>
                                    <overWrite>true</overWrite>
